template(name="datasetDetail")
  .container
    .row
      .col-sm-12
        .dataset-detail
          +with dataset
            h1.inline
              span #{name}
            +with file
              if isUploaded
                a.btn.btn-primary.pull-right(href="{{this.url download=true}}" target="_blank") Download
              else
                p Upload in progress
